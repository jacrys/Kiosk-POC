{
  "extends": "@vue/tsconfig/tsconfig.json",
  "files": [],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/types",
      "./node_modules/vuetify/types"
    ],
    "strict": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["vite/client","vuetify", "vue-router"]
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
